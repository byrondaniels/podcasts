# Copy this file to terraform.tfvars and fill in your values

environment      = "dev"
s3_bucket_name   = "your-podcast-audio-bucket"
mongodb_uri      = "mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority"
mongodb_db_name  = "podcast_db"
aws_region       = "us-east-1"
log_retention_days = 14

# This will be set after first Docker image push
# lambda_image_uri = "123456789012.dkr.ecr.us-east-1.amazonaws.com/podcast-chunking-lambda:latest"
